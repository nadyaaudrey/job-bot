<!--Bookmarks entries-->
<div style="margin-top:20px">
{{#each jobs}}
{{#if Bookmarked}}
<a href="/bookmark_jobDesc">
<div class="media border">
    <div class="media-left">
        <img src={{img}} width="100" height="100">
    </div>
    <div class="media-body">
        <p class="media-heading">{{Position}}</p>
        <p>{{Company}}</p>
        <p>{{Location}}</p>
    </div>
    <span class="media-right fa fa-bookmark fa-2x" style="margin-right: 50px; margin-top: 50px"></span>
</div>
</a>
{{/if}}
{{/each}}
</div>

